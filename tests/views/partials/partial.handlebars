<p>Content from the partial:</p>

<table>
    <thead>
    <tr>
        <th>item</th><th>value</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td>model variable</td>
        <td class="variable">{{myvar}}</td>
    </tr>
    <tr>
        <td>query variable</td>
        <td class="query">{{req.query.myvar}}</td>
    </tr>
    <tr>
        <td>jsonify</td>
        <td class="jsonify">{{{jsonify json}}}</td>
    </tr>
    <tr>
        <td>markdown</td>
        <td class="markdown">{{{md markdown}}}</td>
    </tr>
    <tr>
        <td>Testing Equal Variables</td>
        <td class="equal">{{#equals json.baz json.qux}}true{{else}}false{{/equals}}</td>
    </tr>
    <tr>
        <td>Testing Equal Variable and String</td>
        <td class="equal">{{#equals json.foo "bar"}}true{{else}}false{{/equals}}</td>
    </tr>
    <tr>
        <td>Testing Unequal Variable and String</td>
        <td class="unequal">{{#equals "nonsense" json.qux}}true{{else}}false{{/equals}}</td>
    </tr>
    </tbody>
</table>